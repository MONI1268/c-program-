  #include<stdio.h>
int main()
{
    int n,k,even=0,odd=0,i;
    printf("enter limits\n");
    scanf("%d",&n);
    printf("enter number\n");
    scanf("%d",&k);
    for(i=1;i<=n;i++)
    {
        if(i%k==0)
        {
            printf("%d\n",i);
            if(i%2==0)
            {
                even=even+i;
            }
            else
            {
                odd=odd+i;
            }
        }
    }
    printf("%d%d",even,odd);
    return 0;
}
